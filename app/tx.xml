<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE datingApp [
  <!ELEMENT datingApp (users, messages, likes)>
  <!ELEMENT users (user+)>
  <!ELEMENT user (id, name, email, gender, age, profilePic)>
  <!ELEMENT id (#PCDATA)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ELEMENT gender (#PCDATA)>
  <!ELEMENT age (#PCDATA)>
  <!ELEMENT profilePic (#PCDATA)>
  
  <!ELEMENT messages (message+)>
  <!ELEMENT message (id, senderId, receiverId, content, timestamp)>
  <!ELEMENT senderId (#PCDATA)>
  <!ELEMENT receiverId (#PCDATA)>
  <!ELEMENT content (#PCDATA)>
  <!ELEMENT timestamp (#PCDATA)>

  <!ELEMENT likes (like+)>
  <!ELEMENT like (userId, likedUserId)>
  <!ELEMENT userId (#PCDATA)>
  <!ELEMENT likedUserId (#PCDATA)>
]>
<datingApp>
  <users>
    <user>
      <id>1</id>
      <name>Alice</name>
      <email>alice@example.com</email>
      <gender>Female</gender>
      <age>25</age>
      <profilePic>alice.jpg</profilePic>
    </user>
    <user>
      <id>2</id>
      <name>Bob</name>
      <email>bob@example.com</email>
      <gender>Male</gender>
      <age>28</age>
      <profilePic>bob.jpg</profilePic>
    </user>
  </users>
  
  <messages>
    <message>
      <id>1</id>
      <senderId>1</senderId>
      <receiverId>2</receiverId>
      <content>Hello Bob, how are you?</content>
      <timestamp>2024-12-20T10:00:00</timestamp>
    </message>
  </messages>
  
  <likes>
    <like>
      <userId>1</userId>
      <likedUserId>2</likedUserId>
    </like>
  </likes>
</datingApp>
